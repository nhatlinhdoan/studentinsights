{
  "env": {
    "browser": true,
    "es6": true
  },
  "globals": {
    "_": true,
    "moment": true,
    "$": true,
    "React": true,
    "describe": true,
    "beforeEach": true,
    "afterEach": true,
    "jasmine": true,
    "expect": true,
    "it": true,
    "d3": true,
    "Highcharts": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "plugins": ["react"],
  "extends": [
    "airbnb"
  ],
  "rules": {
    "no-restricted-properties": [2, {
      "object": "Object",
      "property": "assign",
      "message": "Not supported by IE 11, try _.clone."
    }]
  }
}
